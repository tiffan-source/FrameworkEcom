{% block body %}
    <h1>Editer la categorie {{item.name}}</h1>

    <form method="post" action="{{path(routePrefix ~  '.edit', {id : item.id})}}">

        {{ field('name', item.name, 'Titre de la categorie') }}
        {{csrf_input()}}

        <div>
            <button type="submit">Soumettre</button>
        </div> 
    </form>
{% endblock %}